			p_to_left: 0,
			p_single_fake: 0,
			p_single_left: 0
		};



		/**
		 * get and validate config
		 */
		var $inp = this.$cache.input,
			val = $inp.prop("value"),
			config, config_from_data, prop;

		// default config
		config = {
			skin: "flat",
			type: "single",

			min: 10,
			max: 100,
			from: null,
			to: null,
