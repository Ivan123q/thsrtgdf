			big: [],
			big_w: [],
			big_p: [],
			big_x: []
		};

		// storage for labels measure variables
		this.labels = {
			// width
			w_min: 0,
			w_max: 0,
			w_from: 0,
			w_to: 0,
			w_single: 0,

